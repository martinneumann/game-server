    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@types/node": "^13.13.0",
    "@types/socket.io-client": "^1.4.35",
    "@types/uuid": "^7.0.3",
    "express": "^4.17.1",
    "i": "^0.3.6",
    "mongodb": "^3.6.4",
    "net": "^1.0.2",
    "npm": "^7.6.0",
    "pixi.js": "^5.3.8",
    "rxjs": "^6.6.6",
    "socket.io": "^3.1.2",
